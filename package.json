{
  "name": "square-chrome-extension",
  "version": "1.0.0",
  "description": "Advanced Chrome extension for Square Dashboard automation with AI-powered agents",
  "main": "dist/background.js",
  "type": "module",
  "scripts": {
    "dev": "vite build --mode development --watch",
    "build": "tsc --noEmit && vite build",
    "build:prod": "tsc --noEmit && vite build --mode production",
    "lint": "eslint src --ext .ts,.tsx --fix",
    "lint:check": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:integration": "playwright test",
    "package": "pnpm run build:prod && node scripts/package.js",
    "prepare": "pnpm audit --audit-level moderate"
  },
  "volta": {
    "node": "20.11.0",
    "pnpm": "8.15.1"
  },
  "keywords": [
    "chrome-extension",
    "square",
    "automation",
    "seo",
    "catalog-management",
    "ai-agents"
  ],
  "author": "Developer",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/chrome": "^0.0.253",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vitest": "^1.1.0"
  },
  "dependencies": {
    "@openai/agents": "^0.0.4",
    "zod": "^3.22.4"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "packageManager": "pnpm@8.15.1",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
